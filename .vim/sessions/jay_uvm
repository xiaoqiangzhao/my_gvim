badd +0 jay_reference/zmk_ref_model.sv
badd +0 jay_scoreboard/zmk_scoreboard.sv
argglobal
silent! argdel *
set stal=2
edit jay_agent/zmk_agent.sv
set splitbelow splitright
wincmd t
set winheight=1 winwidth=1
argglobal
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let s:l = 20 - ((9 * winheight(0) + 11) / 23)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
20
normal! 019|
